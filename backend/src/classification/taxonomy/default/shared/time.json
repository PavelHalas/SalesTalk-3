    {"match": ["last 6 months"], "window": "l6m", "granularity": "month"},
    {"match": ["last 12 months"], "window": "l12m", "granularity": "month"},
    {"match": ["last 8 quarters"], "window": "l8q", "granularity": "quarter"},
    {"match": ["last 30 days"], "window": "l30d", "granularity": "day"},
    {"match": ["last 90 days"], "window": "l90d", "granularity": "day"},
    {"match": ["this quarter", "current quarter"], "period": "this_quarter", "granularity": "quarter"}
